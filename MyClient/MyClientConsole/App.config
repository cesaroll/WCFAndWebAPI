<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IMyService" />
                <binding name="BasicHttpBinding_IMulService" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:49037/MyService.svc" 
                      binding="basicHttpBinding"
                      bindingConfiguration="BasicHttpBinding_IMyService" 
                      contract="MyServiceReference.IMyService"
                      name="BasicHttpBinding_IMyService" />
            <endpoint address="http://localhost:21951/MyMulServiceHost.svc"
                      binding="basicHttpBinding" 
                      bindingConfiguration="BasicHttpBinding_IMulService"
                      contract="MulServiceReference.IMulService" 
                      name="BasicHttpBinding_IMulService" />
        </client>
    </system.serviceModel>
</configuration>